<Window x:Class="TheEnterprise.Pages.Admin.PopUp.QuestionGroup.CreateQuestianGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CreateQuestianGroup" Height="371" Width="400">
    <Grid>
        <StackPanel>
            <Label Content="Opret spørgsmålsgruppe" FontSize="15" Margin="0,0,174,0" />
            <Label x:Name="createFeedbackLabel" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Width="392" Foreground="Red"/>
            <StackPanel Orientation="Horizontal" Height="249">
                <StackPanel Width="196" >
                    <Label Height="25" Content="Titel" Margin="0,0,0,5" />
                    <Label Height="25"  Content="Rank" Margin="0,0,0,5" />
                    <Label  Height="25" Content="Info" Margin="0,0,0,5" />
                </StackPanel>
                <StackPanel Width="180" >
                    <TextBox Height="25" x:Name="QuestionGroupTitleTextBox" Margin="0,0,0,5"  />
                    <TextBox Height="25" x:Name="QuestionRankGroupTextBox" Margin="0,0,0,5" />
                    <TextBox x:Name="QuestionInfoGroupTextBox" AcceptsReturn="True" TextWrapping="Wrap" Height="161" Margin="0,0,0,5" />
                </StackPanel>
            </StackPanel>
            <Button Height="25" Width="70" Content="Gem" Click="Button_Click" Margin="161,0" />
        </StackPanel>

    </Grid>
</Window>
